    <copy file="${basedir}/web/WEB-INF/lib/jersey-common.jar" tofile="${artifact.output.optimized-firewall-anomaly-resolution:war_exploded}/WEB-INF/lib/jersey-common.jar"/>
    <copy file="${basedir}/web/WEB-INF/lib/jersey-server.jar" tofile="${artifact.output.optimized-firewall-anomaly-resolution:war_exploded}/WEB-INF/lib/jersey-server.jar"/>
