      <fileset dir="${optimized-firewall-anomaly-resolution.output.dir}"/>
    </copy>
    <mkdir dir="${artifact.output.optimized-firewall-anomaly-resolution:war_exploded}/WEB-INF/lib"/>
    <copy file="${basedir}/lib/junit-4.13.jar" tofile="${artifact.output.optimized-firewall-anomaly-resolution:war_exploded}/WEB-INF/lib/junit-4.13.jar"/>
    <copy file="${basedir}/lib/hamcrest-core-1.3.jar" tofile="${artifact.output.optimized-firewall-anomaly-resolution:war_exploded}/WEB-INF/lib/hamcrest-core-1.3.jar"/>
