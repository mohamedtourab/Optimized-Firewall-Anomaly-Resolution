    <copy file="${basedir}/web/WEB-INF/lib/hk2-api-2.6.1.jar" tofile="${artifact.output.optimized-firewall-anomaly-resolution:war_exploded}/WEB-INF/lib/hk2-api-2.6.1.jar"/>
